{
    "name": "pragmarx/firewall",

    "description": "A Laravel 4.1+ IP whitelisting and blacklisting",

    "keywords": ["firewall", "whitelist", "blacklist", "laravel"],

    "license": "BSD-3-Clause",

    "authors": [
        {
            "name": "Antonio Carlos Ribeiro",
            "email": "acr@antoniocarlosribeiro.com",
            "role": "Creator"
        }
    ],

    "config": {
        "preferred-install": "dist",
        "minimum-stability": "dev",
        "prefer-stable": false
    },
    
    "repositories": [
        {
            "url": "https://github.com/younes0/firewall.git",
            "type": "git"
        }
    ],
    
    "require": {
        "php": ">=5.3.7",
        "illuminate/support": "~4|~5",
        "illuminate/cache": "~4|~5",
        "illuminate/filesystem": "~4|~5",
        "pragmarx/support": "dev-master"
    },

    "suggest": {
        "geoip/geoip": "~1.14"
    },

    "autoload": {
        "psr-4": {
            "PragmaRX\\Firewall\\": "src/"
        }
    },

    "extra": {
        "branch-alias": {
            "dev-master": "0.5.x-dev"
        }
    }
}
